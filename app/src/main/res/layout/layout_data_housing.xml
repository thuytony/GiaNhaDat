<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <fr.ganfra.materialspinner.MaterialSpinner
        android:id="@+id/spinnerCity"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:ms_alignLabels="true"
        app:ms_alwaysShowFloatingLabel="true"
        app:ms_enableFloatingLabel="true"
        app:ms_arrowColor="@color/dropDownArrowColor"
        app:ms_arrowSize="18dp"
        app:ms_baseColor="@color/colorPrimaryDark"
        app:ms_dropDownHintView="@layout/dropdown_hint_item"
        app:ms_enableErrorLabel="true"
        app:ms_errorColor="@color/errorColor"
        app:ms_floatingLabelColor="@color/colorPrimaryDark"
        app:ms_floatingLabelText="@string/spinner_city"
        app:ms_highlightColor="@color/colorPrimaryDark"
        app:ms_multiline="false"
        app:ms_thickness="1dp"
        app:ms_typeface="fonts/DroidSerif-Regular.ttf"
        app:ms_hintColor="@color/colorGrayText"
        app:ms_hintView="@layout/layout_text_spinner_hint"/>
    <fr.ganfra.materialspinner.MaterialSpinner
        android:id="@+id/spinnerDistrict"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerCity"
        app:ms_alignLabels="true"
        app:ms_alwaysShowFloatingLabel="true"
        app:ms_enableFloatingLabel="true"
        app:ms_arrowColor="@color/dropDownArrowColor"
        app:ms_arrowSize="18dp"
        app:ms_baseColor="@color/colorPrimaryDark"
        app:ms_dropDownHintView="@layout/dropdown_hint_item"
        app:ms_enableErrorLabel="true"
        app:ms_errorColor="@color/errorColor"
        app:ms_floatingLabelColor="@color/colorPrimaryDark"
        app:ms_floatingLabelText="@string/spinner_district"
        app:ms_highlightColor="@color/colorPrimaryDark"
        app:ms_multiline="false"
        app:ms_thickness="1dp"
        app:ms_typeface="fonts/DroidSerif-Regular.ttf"
        app:ms_hintColor="@color/colorGrayText"
        app:ms_hintView="@layout/layout_text_spinner_hint"/>
    <fr.ganfra.materialspinner.MaterialSpinner
        android:id="@+id/spinnerStreet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintLeft_toRightOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinnerDistrict"
        app:ms_alignLabels="true"
        app:ms_alwaysShowFloatingLabel="true"
        app:ms_enableFloatingLabel="true"
        app:ms_arrowColor="@color/dropDownArrowColor"
        app:ms_arrowSize="18dp"
        app:ms_baseColor="@color/colorPrimaryDark"
        app:ms_dropDownHintView="@layout/dropdown_hint_item"
        app:ms_enableErrorLabel="true"
        app:ms_errorColor="@color/errorColor"
        app:ms_floatingLabelColor="@color/colorPrimaryDark"
        app:ms_floatingLabelText="@string/spinner_street"
